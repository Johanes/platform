<template xmlns="http://ws.apache.org/ns/synapse" name="import_csv">
	<parameter name="worksheet.name"/>	
	<parameter name="spreadsheet.name"/>
	<parameter name="csv.name" />
	<parameter name="batch.enable" />	
	<parameter name="batch.size" />	
	<sequence>
		<log level="custom">
			<property name="FLW" value="GoogleSpreadsheet : Import CSV"/>
			<property name="worksheet.name" expression="$ctx:worksheet.name"/>
			<property name="spreadsheet.name" expression="$ctx:spreadsheet.name"/>
			<property name="csv.name" expression="$ctx:csv.name"/>
			<property name="batch.enable" expression="$ctx:batch.enable"/>	
			<property name="batch.size" expression="$ctx:batch.size"/>			
		</log>		
		<class
			name="org.wso2.carbon.connector.googlespreadsheet.GoogleSpreadsheetImportCSV" />
	</sequence>
</template>	