<template xmlns="http://ws.apache.org/ns/synapse" name="search_cell">
	<parameter name="worksheet.name"/>	
	<parameter name="spreadsheet.name"/>
	<parameter name="search.string" />
	<sequence>
		<log level="custom">
			<property name="FLW" value="GoogleSpreadsheet : Search Cell"/>
			<property name="worksheet.name" expression="$ctx:worksheet.name"/>
			<property name="spreadsheet.name" expression="$ctx:spreadsheet.name"/>
			<property name="search.string" expression="$ctx:search.string"/>
		</log>
		<class
			name="org.wso2.carbon.connector.googlespreadsheet.GoogleSpreadsheetSearchCell" />
	</sequence>
</template>	