<template xmlns="http://ws.apache.org/ns/synapse" name="update_worksheet_metadata">
    <parameter name="spreadsheet.name" />
	<parameter name="worksheet.old.name"/>	
	<parameter name="worksheet.new.name"/>	
	<parameter name="worksheet.rows"/>
	<parameter name="worksheet.columns"/>
	<sequence>
		<log level="custom">
			<property name="FLW" value="GoogleSpreadsheet : Update Worksheet Metdata"/>
			<property name="spreadsheet.name" expression="$ctx:spreadsheet.name"/>
			<property name="worksheet.old.name" expression="$ctx:worksheet.old.name"/>
			<property name="worksheet.new.name" expression="$ctx:worksheet.new.name"/>
			<property name="worksheet.rows" expression="$ctx:worksheet.rows"/>
			<property name="worksheet.columns" expression="$ctx:worksheet.columns"/>
		</log>
		<class
			name="org.wso2.carbon.connector.googlespreadsheet.GoogleSpreadsheetUpdateWorksheetMetadata" />
	</sequence>
</template>	