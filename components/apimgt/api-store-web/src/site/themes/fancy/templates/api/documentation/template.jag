<% jagg.template("api/documentation", function(inputs, outputs, jagg) { %>
<div class="api-documentation">
    <%
    var i, type, length, doc, docs, empty = true, documentations = outputs.documentations;
    for(type in documentations) {
        if(documentations.hasOwnProperty(type)) {
            empty = false;
            docs = documentations[type];
    %>
    <div class="accordion-group">
        <div class="accordion-heading">
            <a class="accordion-toggle"><%=type%></a>
        </div>
        <div class="accordion-body">
            <div class="accordion-inner">
                <%
                length = docs.length;
                for(i = 0; i < length; i++) {
                    doc = docs[i];
                %>
                <div class="documentation">
                    <h5>Name:</h5>
                    <p><%=doc.name%></p>
                    <div class="documentaion-body"><h5>Summary:</h5><p><%=doc.summary%></p>
                    <%if(doc.sourceUrl){%>
                    <h5>Documentation URL:</h5><p><a href="<%=doc.sourceUrl%>"><%=doc.sourceUrl%></a></p>
                    <%}%>
                    </div>
                    <hr/>
                </div>
                <% } %>
            </div>
        </div>
    </div>
    <%  } %>
    <% } %>
    <% if(empty) { %><p>No documentations</p><% } %>
</div>
<% }); %>