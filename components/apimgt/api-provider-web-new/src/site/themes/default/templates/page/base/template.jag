        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-popover.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-button.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-collapse.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-carousel.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-typeahead.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jqplot/plugins/jqplot.dateAxisRenderer.min.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jqplot/plugins/jqplot.canvasTextRenderer.min.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jqplot/plugins/jqplot.canvasAxisTickRenderer.min.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jqplot/plugins/jqplot.categoryAxisRenderer.min.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jqplot/plugins/jqplot.barRenderer.min.js"></script>

        <script src="http://malsup.github.com/jquery.form.js"></script>



        <%
        var context = site.context;

        var insertCode = function(list, fn) {
        var obj, tmpl, key, path;
        for(tmpl in list) {
        if(list.hasOwnProperty(tmpl)) {
        obj = list[tmpl];
        for(key in obj) {
        if(obj.hasOwnProperty(key)) {
        path = "/" + context + obj[key];
        fn(path);
        }
}
}
}
};

insertCode(jagg.global.js, function(path) {
        print('<script type="text/javascript" src="' + path + '"></script>');
        });

insertCode(jagg.global.css, function(path) {
        print('<link type="text/css" rel="stylesheet" href="' + path + '"/>');
        });

insertCode(jagg.global.code, function(code) {
        print(code);
        });
%>
    </head>
        <body>

            <div id="header">
              <% jagg.includeTemplate("header", null); %>
            </div>

            <div class="container-fluid content-section">
            <div class="row-fluid">
               <% jagg.includeTemplates(data.body); %>
             </div>
             <hr>
