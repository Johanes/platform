    <!--Loca styles-->
        <link href="<%=site.context%>/site/themes/default/localcss/localstyles.css" rel="stylesheet">
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/jquery-1.7.1.min.js"></script>

        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-transition.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-transition.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-alert.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-modal.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-dropdown.js"></script>
        <script type="text/javascript" src="<%=site.context%>/site/themes/default/lib/bootstrap/js/bootstrap-scrollspy.js"></script>
