<%
var updateAPI = function (api) {
    var log = new Log(),
            provider = jagg.module("greg").getAPIProviderObj();
    try {
        var success = provider.updateAPI(api);
        if (log.isDebugEnabled()) {
            log.debug("updateAPI : " + api.name + "-" + api.version);
        }
        return {
            error:success
        };
    } catch (e) {
        log.error(e.message);
        return {
            error:e

        };
    }
};
%>