<analyzerSequence name="KeyUsageSummarySequence">
    <trigger cron="0/10 * * * * ? *"/>
    <analyzers>
        <get name="BASEEnriched" batchSize="1000"/>
        <groupBy>
            <field name="context"/>
            <field name="consumerKey"/>
            <field name="userId"/>
        </groupBy>
        <drop type="column">
            <fieldSet>
                <field name="api_version"/>
                <field name="api"/>
                <field name="version"/>
                <field name="resource"/>
                <field name="method"/>
                <field name="requestTime"/>
                <field name="response"/>
                <field name="responseTime"/>
                <field name="serviceTime"/>
            </fieldSet>
        </drop>
        <aggregate>
            <measure name="request" aggregationType="SUM"/>
        </aggregate>
        <put name="KeyUsageSummaryTable" dataSourceType="CASSANDRA">
            <onExist>
                <aggregate>
                    <measure name="request" aggregationType="SUM"/>
                </aggregate>
            </onExist>
        </put>
    </analyzers>
</analyzerSequence>