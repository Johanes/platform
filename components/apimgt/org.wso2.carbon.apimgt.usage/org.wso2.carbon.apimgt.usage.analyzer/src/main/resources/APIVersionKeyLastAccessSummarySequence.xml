<analyzerSequence name="APIVersionKeyLastAccessSummarySequence">
    <trigger cron="1 * * * * ? *"/>
    <analyzers>
        <get name="BASEEnriched" batchSize="1000"/>
        <groupBy>
            <field name="api_version"/>
            <field name="consumerKey"/>
            <field name="userId"/>
            <field name="context"/>
        </groupBy>
        <drop type="column">
            <fieldSet>
                <field name="api"/>
                <field name="version"/>
                <field name="resource"/>
                <field name="method"/>
                <field name="request"/>
                <field name="response"/>
                <field name="responseTime"/>
                <field name="serviceTime"/>
            </fieldSet>
        </drop>
        <aggregate>
            <measure name="requestTime" aggregationType="MAX"/>
        </aggregate>
        <put name="APIVersionKeyLastAccessSummaryTable" dataSourceType="CASSANDRA">
            <onExist>
                <aggregate>
                    <measure name="requestTime" aggregationType="MAX"/>
                </aggregate>
            </onExist>
        </put>
    </analyzers>
</analyzerSequence>