<%
var addApplication = function (username, application) {
    var log = new Log(),
            store = require("/core/greg/greg.js").getAPIStoreObj();

    try {
        store.addApplication(application, username);
        if (log.isDebugEnabled()) {
            log.debug("addApplication");
        }
        return {
            error:false
        };
    } catch (e) {
        log.error(e.message);
        return {
            error:e
        };
    }
};
%>