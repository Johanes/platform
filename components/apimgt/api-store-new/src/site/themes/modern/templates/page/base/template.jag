<% jagg.template("page/base", function(inputs, outputs, jagg) { %>
<!DOCTYPE html>
<html>
<head>
    <title><%=outputs.title%></title>
    <meta charset="UTF-8">
    <meta name="description" content=""/>

    <script type="text/javascript" src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/jquery-1.7.1.min.js")%>"></script>
    <!--[if lt IE 9]><script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/html5/html5.js")%>"></script><![endif]-->
    <script type="text/javascript" src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/kickStart/prettify.js")%>"></script>
    <script type="text/javascript" src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/kickStart/kickstart.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/external/jquery.bgiframe-2.1.2.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.core.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.widget.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.mouse.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.button.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.draggable.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.position.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.resizable.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.ui.dialog.min.js")%>"></script>
    <script src="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/ui/jquery.effects.core.min.js")%>"></script>

    <link rel="stylesheet" type="text/css" href="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/kickStart/css/kickstart.css")%>" media="all"/>
    <link rel="stylesheet" type="text/css" href="<%=jagg.getAbsoluteUrl("/site/themes/modern/js/lib/jquery/css/start/jquery-ui-1.8.19.custom.css")%>"/>
    <link rel="stylesheet" type="text/css" href="<%=jagg.getAbsoluteUrl("/site/themes/modern/css/style.css")%>" media="all"/>
    <%
    var data = jagg.data();
    if(data.header) {
        jagg.printData(data.header.js);
        jagg.printData(data.header.css);
        jagg.printData(data.header.code);
    }
    %>
</head>
<body>
<% jagg.includeJag("templates/page/base/dialog-box.jag"); %>
<div id="header">
    <a id="top-of-page"></a>
    <% jagg.includeBlock("menu/primary", null); %>
</div>

<div class="clearfix wrap">
    <% jagg.includeBlocks(outputs.body); %>
    <div class="clear"></div>

    <div id="footer">
        &copy; Copyright 2011â€“2012 All Rights Reserved.
        <a id="link-top" href="#top-of-page">Top</a>
    </div>
</div>
<%
if(data.footer) {
    jagg.printData(data.footer.js);
    jagg.printData(data.footer.css);
    jagg.printData(data.footer.code);
}
%>
</body>
</html>
<% }); %>