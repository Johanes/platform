<% jagg.template("search/api-search", function(inputs, outputs, jagg) { %>
<div class="col_12 search-wrapper">
    <div class="col_3"></div>
    <div class="col_9">
        <h1>APIs</h1>
        <form action="<%=jagg.getMappedUrl("/site/pages/list-apis.jag")%>">
            <input type="text" class="col_6" name="query" placeholder="Search APIs"/>
            <button class="medium"><span class="icon" data-icon="s"></span>Search</button>
        </form>
    </div>
</div>
<% }); %>