<% jagg.template("search/api-search", function(inputs, outputs, jagg) { %>
        <div class="header-section">
        <div class="row">
            <div class="span6 offset3">
                <h1>APIs</h1>

                <p>The practice of publishing APIs has allowed web communities to create an open architecture for sharing content and data between communities and applications.</p>

            </div>
            <div class="span3">
                <div class="input-append pull-left">
                    <form action="<%=jagg.getMappedUrl("/site/pages/list-apis.jag")%>">
                    <input id="appendedInputButton" size="16" class="span2" name="query" placeholder="Search APIs" type="text"><button class="btn btn-primary search-button" type="button"><i class="icon-search icon-white"></i></button>
                    </form>
                </div>
            </div>
        </div>
    </div>

<% }); %>