<ruleService name="RuleTestService2">
    <description>RuleTestService2</description>
    <ruleset>
        <x><![CDATA[
           package RoutingRules2;

           import org.wso2.carbon.rule.samples.Result;
           import org.wso2.carbon.rule.samples.Symbol;
           global java.lang.String source

           rule InvokeIBM

           when
           $symbol : Symbol( symbol == "IBM" )
           then
           Result result = new Result();
           result.setSymbol("WSO2");
           result.setPrice(925.2193664813152);
           insert(result);
           end

           rule InvokeSUN

           when
           $symbol : Symbol( symbol == "SUN" )
           then
           $symbol.setSymbol("MSFT");
           end
           ]]>
        </x>
    </ruleset>
    <operation name="getQuote">
        <with-param type="org.wso2.carbon.rule.samples.Symbol"/>
        <result>
            <element type="org.wso2.carbon.rule.samples.Result"/>
        </result>
    </operation>
    <operation name="getQuote2">
        <with-param type="org.wso2.carbon.rule.samples.Symbol"/>
        <result>
            <element type="org.wso2.carbon.rule.samples.Symbol"/>
        </result>
    </operation>
</ruleService>