<configuration xmlns="http://www.wso2.org/products/rule/jsr94">
    <provider class="Class" uri="URI">
        <property name="propertyOne" value="one"/>
        <property xmlns:c="http://services.samples/xsd" name="propertytwo" expression="//c:xpath"/>
    </provider>
    <executionSet uri="some uri">
        <source key="keyof script"/>
        <creation>
            <property name="propertyOne" value="one"/>
            <property xmlns:c="http://services.samples/xsd" name="propertytwo"
                      expression="//c:xpath"/>
        </creation>
        <registration>
            <property name="propertyOne" value="one"/>
            <property xmlns:c="http://services.samples/xsd" name="propertytwo"
                      expression="//c:xpath"/>
        </registration>
        <deregistration>
            <property name="propertyOne" value="one"/>
            <property xmlns:c="http://services.samples/xsd" name="propertytwo"
                      expression="//c:xpath"/>
        </deregistration>
    </executionSet>
    <session type="stateful">
        <property name="propertyOne" value="one"/>
        <property xmlns:c="http://services.samples/xsd" name="propertytwo" expression="//c:xpath"/>
    </session>
    <input xmlns:c="http://services.samples/xsd" name="DomEvent" expression="//c:xpath" type="dom"/>
    <input name="InputTwo" class="A.Example" type="pojo"/>
    <input name="inputOone" value="12" type="int"/>
    <output xmlns:c="http://services.samples/xsd" name="DomEvent" expression="//c:xpath"
            type="dom"/>
    <output name="InputTwo" class="A.Example" type="pojo"/>
    <output name="inputOone" key="keyone" type="int"/>
    <inputFilter class="A class Name"/>
    <outputFilter class="A class Name"/>
</configuration>