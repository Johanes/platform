        <axis2.wso2.version.humantask>${axis2.wso2.version}</axis2.wso2.version.humantask>
        <saxon-bps.wso2.version.humantask>9.0.0.x-wso2v1</saxon-bps.wso2.version.humantask>
        <wso2carbon.version.humantask>${wso2carbon.version}</wso2carbon.version.humantask>
        <simple-json.version>1.1.wso2v1</simple-json.version>
    </properties>
</project>

