    <h3>Entry</h3>

        <pre class="prettyprint linenums">
&lt%
//creating entries for feed
var firstEntry = new Entry();
firstEntry .title = "Jaggery Feed";
firstEntry .content= "jaggery is server side .....";

var Secondentry = new Entry();
Secondentry .title = "Jaggery Entry";
Secondentry .content= "make feed entry to be create .....";

//creating list of entris
var entryList  = new Array();
entryList[0]  = firstEntry ;
entryList[1]  = Secondentry ;

//creating feed
var feed= new Feed();
//adding new entries for Feed
feed.entries=entryList;
print(feed.entries[0].title);
print('&ltbr&gt');
print(feed.entries[1].title);
%&gt
</pre>
    <%
    var reqCode = '&lt%'
    + '\n //creating entries for feed'
    + '\n var firstEntry = new Entry();'
    + '\n firstEntry .title = "Jaggery Feed";'
    + '\n firstEntry .content= "jaggery is server side .....";'
    + '\n var Secondentry = new Entry();'
    + '\n Secondentry .title = "Jaggery Entry";'
    + '\n Secondentry .content= "make feed entry to be create .....";'
    + '\n //creating list of entris'
    + '\n var entryList  = new Array();'
    + '\n entryList[0]  = firstEntry ;'
    + '\n entryList[1]  = Secondentry ;'
    + '\n //creating feed'
    + '\n var feed= new Feed();'
    + '\n //adding new entries for Feed'
    + '\n feed.entries=entryList;'
    + '\n print(feed.entries[0].title);'
    + '\n print(feed.entries[1].title);'
    + '\n %&gt';

    var codeSnip = encodeURIComponent(reqCode);
    var urlString = '../tryitframe.jag?code='+codeSnip;

    include('/includes/tryit_but.jag');
    %>

    <div id="pageLinks">
                <a class="navilink" href="#overview">Overview</a>
                <a class="navilink" href="#members">Members</a>
                <a class="navilink" href="#operations">Operations</a>
                </div>
                 <section id="overview">
                     <h4>Overview</h4>
                      <p class="description">Entry object is the container for entries returned by Feed.</p>
                     </section>

    <section id="members">
        <h4>Members</h4>

        <table class="clsStd members-table">
            <tbody>
            <tr>
                <th>Member</th>
                <th>Type</th>
                <th>Description</th>
            </tr>
            <tr>
                <td>author</td>
                <td>String </td>
                <td>The author of this entry.</td>
            </tr>
            <tr>
                <td>category</td>
                <td>String </td>
                <td>The category of this entry.</td>
            </tr>
            <tr>
                <td>content</td>
                <td>String </td>
                <td>The content of this entry.</td>
            </tr>
            <tr>
                <td>contributor</td>
                <td>String </td>
                <td>The contributor of this entry.</td>
            </tr>
            <tr>
                <td>id</td>
                <td>String </td>
                <td>The id of this entry.</td>
            </tr>
            <tr>
                <td>link</td>
                <td>String[] </td>
                <td>The location(s) of this entry.</td>
            </tr>
            <tr>
                <td>published</td>
                <td>String </td>
                <td>The date this entry was published.</td>
            </tr>
            <tr>
                <td>rights</td>
                <td>String </td>
                <td>The rights associated with this entry.</td>
            </tr>
            <tr>
                <td>summary</td>
                <td>String </td>
                <td>The summary of this entry.</td>
            </tr>
            <tr>
                <td>title</td>
                <td>String </td>
                <td>The title of this entry.</td>
            </tr>
            <tr>
                <td>updated</td>
                <td>String </td>
                <td>The date this entry was updated.</td>
            </tr>

            </tbody>
        </table>
    </section>



