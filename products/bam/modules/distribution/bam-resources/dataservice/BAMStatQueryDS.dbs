    <query id="getLatestDataForServicesQuery" useConfig="bamDataSource">
        <sql>SELECT BSD.BAM_SERVICE_ID AS SERVICE_ID, BSD.BAM_AVG_RES_TIME AS AVG_RES_TIME, BSD.BAM_MAX_RES_TIME AS MAX_RES_TIME, BSD.BAM_MIN_RES_TIME AS MIN_RES_TIME, BSD.BAM_CUM_REQ_COUNT AS REQ_COUNT, BSD.BAM_CUM_RES_COUNT AS RES_COUNT, BSD.BAM_CUM_FAULT_COUNT AS FAULT_COUNT FROM BAM_SERVICE_DATA BSD INNER JOIN (SELECT BAM_SERVICE_ID, MAX(BAM_ID) AS MAX_ID FROM BAM_SERVICE_DATA WHERE BAM_SERVICE_ID IN (?) GROUP BY BAM_SERVICE_ID) GBSD ON BSD.BAM_SERVICE_ID = GBSD.BAM_SERVICE_ID AND BSD.BAM_ID = GBSD.MAX_ID</sql>
        <param name="serviceID" paramType="ARRAY" sqlType="INTEGER" type=""/>
        <result element="serviceData" rowName="data" defaultNamespace="http://bam.carbon.wso2.org">
            <element name="serviceID" column="SERVICE_ID" />
            <element name="avgTime" column="AVG_RES_TIME" />
            <element name="maxTime" column="MAX_RES_TIME" />
            <element name="minTime" column="MIN_RES_TIME" />
            <element name="reqCount" column="REQ_COUNT" />
            <element name="resCount" column="RES_COUNT" />
            <element name="faultCount" column="FAULT_COUNT" />
