   <!-- Filter criteria for SAP Gadget -->

      <query id="getMessagesForStatusQ" useConfig="bamDataSource">
        <sql>
       SELECT 
            prop.BAM_MESSAGE_ID AS BAM_MESSAGE_ID
            ,prop.BAM_KEY AS BAM_KEY
            ,prop.BAM_VALUE AS BAM_VALUE
            ,data.BAM_TIMESTAMP AS BAM_TIMESTAMP
            ,data.BAM_DIRECTION AS BAM_DIRECTION
            ,data.BAM_STATUS AS BAM_STATUS
            ,msg.BAM_MSG_ID AS BAM_MSG_ID
            ,SVC.BAM_SERVICE_NAME AS BAM_SERVICE_NAME
	    ,data.BAM_ACTIVITY_ID as BAM_ACTIVITY_ID
	    ,msg.BAM_IP_ADDRESS as BAM_IP_ADDRESS
	    ,msg.BAM_TARGET_IP_ADDRESS as BAM_USER_AGENT	
        FROM 
            BAM_MESSAGE_PROPERTIES prop
            ,BAM_MESSAGE_DATA data
            ,BAM_MESSAGE msg
            ,BAM_SERVICE SVC
            ,BAM_OPERATION OP
	    ,BAM_SERVER SVR
        WHERE 
            prop.BAM_MESSAGE_ID  = data.BAM_MESSAGE_ID 
            AND prop.BAM_MESSAGE_ID = msg.BAM_ID
            AND data.BAM_TIMESTAMP BETWEEN ? AND ? 
	    AND SVR.BAM_URL LIKE ?
	    AND SVC.BAM_SERVICE_NAME LIKE ?
	    AND OP.BAM_OP_NAME LIKE ?	
	    AND data.BAM_DIRECTION LIKE ?
	    AND data.BAM_STATUS LIKE ?    
	    AND OP.BAM_ID = msg.BAM_OP_ID  
	    AND SVC.BAM_ID = OP.BAM_SERVICE_ID 
	    AND SVR.BAM_SERVER_ID = SVC.BAM_SERVER_ID 
            AND OP.BAM_ID  = msg.BAM_OP_ID
            AND prop.BAM_MESSAGE_ID 
            IN 
            (
				select A.BAM_MESSAGE_ID 
				from 
					BAM_MESSAGE_PROPERTIES A  
					inner join BAM_MESSAGE_PROPERTIES B
						on A.BAM_MESSAGE_ID = B.BAM_MESSAGE_ID
					inner join BAM_MESSAGE_PROPERTIES C
						on A.BAM_MESSAGE_ID = C.BAM_MESSAGE_ID
					inner join BAM_MESSAGE_PROPERTIES D
						on A.BAM_MESSAGE_ID = D.BAM_MESSAGE_ID
					inner join BAM_MESSAGE_PROPERTIES E
						on A.BAM_MESSAGE_ID = E.BAM_MESSAGE_ID
					inner join BAM_MESSAGE_PROPERTIES F
						on A.BAM_MESSAGE_ID = F.BAM_MESSAGE_ID
					inner join BAM_MESSAGE_PROPERTIES G
