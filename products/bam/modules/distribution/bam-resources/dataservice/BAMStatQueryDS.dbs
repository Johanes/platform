    <!-- get Activity details (operations, services, servers, messages and timestamps) for the given timeRange -->
    <query id="getActivityDetailsForTimeRangeQ" useConfig="bamDataSource">
        <sql>
            SELECT 
            server.BAM_URL AS BAM_SERVER
            , service.BAM_SERVICE_NAME AS SERVICE_NAME
            , opt.BAM_OP_NAME AS OPERATION_NAME
            , msg_data.BAM_MESSAGE_ID AS MESSAGE
            , msg.BAM_TIMESTAMP AS TIME_STAMP
            , msg_data.BAM_DIRECTION AS BAM_DIRECTION
            , msg_data.BAM_STATUS AS STATUS
            , msg.BAM_ACTIVITY_ID AS ACTIVITY
            FROM 
            BAM_MESSAGE msg
            , BAM_ACTIVITY act
            , BAM_OPERATION opt
            , BAM_SERVICE service
            , BAM_SERVER server
            , BAM_MESSAGE_DATA msg_data
            WHERE 
            msg.BAM_TIMESTAMP BETWEEN ? AND ?
            AND act.BAM_ID = msg.BAM_ACTIVITY_ID 
            AND opt.BAM_ID = msg.BAM_OP_ID 
            AND service.BAM_ID = opt.BAM_SERVICE_ID 
            AND server.BAM_SERVER_ID = service.BAM_SERVER_ID 
            AND msg.BAM_ID  = msg_data.BAM_MESSAGE_ID  
            AND ( (NOT (msg_data.BAM_MESSAGE_ID IN (
                            select
                                 propx.BAM_MESSAGE_ID 
                            from
                                 BAM_MESSAGE_PROPERTIES propx )
                 ) ) OR (
                        msg_data.BAM_MESSAGE_ID IN 
                            (
                                select A.BAM_MESSAGE_ID 
                                from 
                                    BAM_MESSAGE_PROPERTIES A  
                                    inner join BAM_MESSAGE_PROPERTIES B
                                    on A.BAM_MESSAGE_ID = B.BAM_MESSAGE_ID
                                    inner join BAM_MESSAGE_PROPERTIES C
                                    on A.BAM_MESSAGE_ID = C.BAM_MESSAGE_ID
                                where 
                                    A.BAM_KEY like ? and A.BAM_VALUE like ?
                                    and B.BAM_KEY like ? and B.BAM_VALUE like ? 
                                    and C.BAM_KEY like ? and C.BAM_VALUE like ?
                            )  

                )
            )
            GROUP BY server.BAM_URL, 
            service.BAM_SERVICE_NAME, 
            opt.BAM_OP_NAME, 
            msg_data.BAM_MESSAGE_ID, 
            msg.BAM_TIMESTAMP, 
            msg_data.BAM_DIRECTION, 
            msg_data.BAM_STATUS, 
            msg.BAM_ACTIVITY_ID

            ORDER BY
            msg.BAM_ACTIVITY_ID, TIME_STAMP
        </sql>
     	<param name="startTime" sqlType="STRING" type=""/>
        <param name="endTime" sqlType="STRING" type=""/>
	<param name="key1" sqlType="STRING" type=""/>
        <param name="value1" sqlType="STRING" type=""/>
	<param name="key2" sqlType="STRING" type=""/>
        <param name="value2" sqlType="STRING" type=""/>
	<param name="key3" sqlType="STRING" type=""/>
        <param name="value3" sqlType="STRING" type=""/>
        <result element="activitiesForTime" rowName="activityForTime" defaultNamespace="http://bam.carbon.wso2.org">
	    <element name="serverUrl" column="BAM_SERVER"/>
	    <element name="serviceName" column="SERVICE_NAME"/>
	    <element name="name" column="OPERATION_NAME"/>
	    <element name="message_name" column="MESSAGE"/>
	    <element name="timestamp" column="TIME_STAMP"/>
	    <element name="direction" column="BAM_DIRECTION"/>
	    <element name="status" column="STATUS"/>
        </result>
    </query>
    <operation name="getActivityDetailsForTimeRange">
        <call-query href="getActivityDetailsForTimeRangeQ">
            <with-param name="startTime" query-param="startTime"/>
            <with-param name="endTime" query-param="endTime"/>
	    <with-param name="key1" query-param="key1"/>
            <with-param name="value1" query-param="value1"/>
	    <with-param name="key2" query-param="key2"/>
