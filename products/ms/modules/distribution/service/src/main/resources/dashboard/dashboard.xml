<resource path="/repository/dashboards" isCollection="true">
	<mediaType />
	<description />
	<children>
		<resource path="/repository/dashboards/mashup" isCollection="true">
			<mediaType />
			<description />
			<children>
				<resource path="/repository/dashboards/mashup/gadgets"
					isCollection="true">
					<mediaType />
					<description />
					<children>
						<resource path="/repository/dashboards/mashup/gadgets/1"
							isCollection="true">
							<mediaType />
							<description />
							<properties>
								<property key="gadgetUrl">
									/registry/resource/_system/config/repository/dashboards/gadgets/gmap.xml
								</property>
							</properties>
							<children>
								<resource path="/repository/dashboards/mashup/gadgets/1/gadgetprefs"
									isCollection="true">
									<mediaType />
									<description />
									<properties>
										<property key="gadgetUserPrefs-1" />
									</properties>
									<children />
								</resource>
							</children>
						</resource>
					</children>
				</resource>
				<resource path="/repository/dashboards/mashup/gadgets"
					isCollection="true">
					<mediaType />
					<description />
					<children>
						<resource path="/repository/dashboards/mashup/gadgets/2"
							isCollection="true">
							<mediaType />
							<description />
							<properties>
								<property key="gadgetUrl">
									/registry/resource/_system/config/repository/dashboards/gadgets/fedex.xml
								</property>
							</properties>
							<children>
								<resource path="/repository/dashboards/mashup/gadgets/2/gadgetprefs"
									isCollection="true">
									<mediaType />
									<description />
									<properties>
										<property key="gadgetUserPrefs-1" />
									</properties>
									<children />
								</resource>
							</children>
						</resource>
					</children>
				</resource>
				<resource path="/repository/dashboards/mashup/gadgets"
					isCollection="true">
					<mediaType />
					<description />
					<children>
						<resource path="/repository/dashboards/mashup/gadgets/3"
							isCollection="true">
							<mediaType />
							<description />
							<properties>
								<property key="gadgetUrl">
									/registry/resource/_system/config/repository/dashboards/gadgets/ebay.xml
								</property>
							</properties>
							<children>
								<resource path="/repository/dashboards/mashup/gadgets/3/gadgetprefs"
									isCollection="true">
									<mediaType />
									<description />
									<properties>
										<property key="gadgetUserPrefs-3" />
									</properties>
									<children />
								</resource>
							</children>
						</resource>
					</children>
				</resource>
				<resource path="/repository/dashboards/mashup/nextGadgetId"
					isCollection="true">
					<mediaType />
					<description />
					<properties>
						<property key="nextGadgetId">2</property>
					</properties>
					<children />
				</resource>
				<!-- Tabs declaration of the MS dashboard -->
				<resource path="/repository/dashboards/mashup/tabs"
					isCollection="true">
					<mediaType />
					<description />
					<properties>
						<property key="nextTabId">1</property>
						<property key="currentTabLayout">0</property>
					</properties>
					<children>
						<resource path="/repository/dashboards/mashup/tabs/0"
							isCollection="true">
							<mediaType />
							<description />
							<properties>
								<property key="tabTitle">Home</property>
							</properties>
							<children>
								<resource path="/repository/dashboards/mashup/tabs/0/currentGadgetLayout"
									isCollection="true">
									<mediaType />
									<description />
									<properties>
										<property key="currentGadgetLayout">G1#1,G2#2,G3#3</property>
									</properties>
									<children />
								</resource>
							</children>
						</resource>
					</children>
				</resource>
			</children>
		</resource>
	</children>
</resource>
