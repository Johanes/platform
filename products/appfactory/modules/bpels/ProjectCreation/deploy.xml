<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:internal.mgt.project.appfactory.carbon.wso2.org="http://internal.mgt.project.appfactory.carbon.wso2.org" xmlns:sample="http://wso2.org/bps/createProject" xmlns:services.registration.user.carbon.wso2.org="http://services.registration.user.carbon.wso2.org">
  <process name="sample:ProjectCreationProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:ProjectCreationProcess" port="ProjectCreationProcessPort"/>
    </provide>
    <invoke partnerLink="ProjectMgtServicePL">
      <service name="internal.mgt.project.appfactory.carbon.wso2.org:ProjectManagementService" port="ProjectManagementServiceHttpsSoap11Endpoint">
<endpoint xmlns="http://wso2.org/bps/bpel/endpoint/config" endpointReference="./../../../../repository/conf/endpoints/ProjectManagementService.epr"/>
</service>
    </invoke>
  </process>
</deploy>
