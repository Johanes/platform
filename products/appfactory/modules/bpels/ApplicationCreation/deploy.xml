<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:sample="http://wso2.org" xmlns:service.mgt.project.appfactory.carbon.wso2.org="http://service.mgt.project.appfactory.carbon.wso2.org" xmlns:service.mgt.repository.svn.appfactory.carbon.wso2.org="http://service.mgt.repository.svn.appfactory.carbon.wso2.org" xmlns:services.mgt.tenant.carbon.wso2.org="http://services.mgt.tenant.carbon.wso2.org">
  <process name="sample:ApplicationCreation">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:ApplicationCreation" port="ApplicationCreationPort"/>
    </provide>
    <invoke partnerLink="TenantMgtPL">
      <service name="services.mgt.tenant.carbon.wso2.org:TenantMgtAdminService" port="TenantMgtAdminServiceHttpsSoap11Endpoint">
      <endpoint xmlns="http://wso2.org/bps/bpel/endpoint/config" endpointReference="./../../../../repository/conf/appfactory/endpoints/TenantMgtService.epr" />
      </service>
    </invoke>
    <invoke partnerLink="ApplicationMgtPL">
      <service name="service.mgt.project.appfactory.carbon.wso2.org:ProjectManagementService" port="ProjectManagementServiceHttpsSoap11Endpoint">
      <endpoint xmlns="http://wso2.org/bps/bpel/endpoint/config" endpointReference="./../../../../repository/conf/appfactory/endpoints/ProjectManagementService.epr" />
      </service>
    </invoke>
    <invoke partnerLink="RepoMgtPL">
      <service name="service.mgt.repository.svn.appfactory.carbon.wso2.org:RepositoryManagementService" port="RepositoryManagementServiceHttpsSoap11Endpoint">
      <endpoint xmlns="http://wso2.org/bps/bpel/endpoint/config" endpointReference="./../../../../repository/conf/appfactory/endpoints/RepoManagementService.epr" />
      </service>
    </invoke>
  </process>
</deploy>