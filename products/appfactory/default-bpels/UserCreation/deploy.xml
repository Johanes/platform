<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:services.registration.user.carbon.wso2.org="http://services.registration.user.carbon.wso2.org" xmlns:wso2.org="http://wso2.org">
  <process name="wso2.org:UserCreationProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="wso2.org:UserCreationProcess" port="UserCreationProcessPort"/>
    </provide>
    <invoke partnerLink="UserRegistrationServicePL">
      <service name="services.registration.user.carbon.wso2.org:UserRegistrationService" port="UserRegistrationServiceHttpsSoap11Endpoint"/>
    </invoke>
    <invoke partnerLink="EmailSenderServicePL">
      <service name="services.registration.user.carbon.wso2.org:EmailSenderService" port="EmailSenderServiceHttpsSoap11Endpoint"/>
    </invoke>
  </process>
</deploy>