<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="GregTestSuite">
    <parameter name="useDefaultListeners" value="false"/>
    <listeners>
        <listener class-name="org.wso2.carbon.automation.core.PlatformTestManager"/>
        <listener class-name="org.wso2.carbon.automation.core.PlatformReportManager"/>
        <listener class-name="org.wso2.carbon.automation.core.PlatformExecutionManager"/>
        <listener class-name="org.wso2.carbon.automation.core.PlatformSuiteManager"/>
    </listeners>
    <test name="Test - GREG">
        <classes>
            <class name="org.wso2.carbon.registry.resource.test.ResourceTestCase"/>
            <class name="org.wso2.carbon.registry.app.test.AtomFeedTestCase"/>
            <class name="org.wso2.carbon.registry.governance.api.test.MetaDataServicesTestCase"/>
            <class name="org.wso2.carbon.registry.version.test.MetaDataTestCase"/>
            <class name="org.wso2.carbon.registry.version.test.CollectionTestCase"/>
        </classes>
    </test>
</suite>